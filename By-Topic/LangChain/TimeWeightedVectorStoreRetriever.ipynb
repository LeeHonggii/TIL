    "# 다양한 decay_rate 값 테스트\n",
    "decay_rates = [0.01, 0.1, 0.5, 0.9, 0.99]\n",
    "\n",
    "print(\"=== Decay Rate 비교 실험 ===\")\n",
    "print(f\"검색어: 'AI'\\n\")\n",
    "\n",
    "for rate in decay_rates:\n",
    "    # 각 decay_rate로 새 리트리버 생성\n",
    "    test_vectorstore = FAISS(\n",
    "        embeddings_model, \n",
    "        faiss.IndexFlatL2(embedding_size), \n",
    "        InMemoryDocstore({}), \n",
