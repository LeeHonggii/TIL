    "        if hours_ago < 24:\n",
    "            time_str = f\"{hours_ago:.1f}시간 전\"\n",
    "        else:\n",
    "            days_ago = hours_ago / 24\n",
    "            time_str = f\"{days_ago:.1f}일 전\"\n",
