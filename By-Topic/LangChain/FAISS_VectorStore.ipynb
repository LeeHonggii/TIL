     "cell_type": "code",
     "execution_count": 25,
     "id": "baf449b2",
     "metadata": {},
     "outputs": [],
     "source": [
       "# 로컬 Disk 에 저장\n",
       "db.save_local(folder_path=\"faiss_db\", index_name=\"faiss_index\")"
     ]
   },
